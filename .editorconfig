# EditorConfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
indent_style = space
indent_size = 4

[{*.{sql,scss,css,twig,yml},package.json}]
indent_size = 2

[*.twig]
insert_final_newline = false

[test/test_data/*]
insert_final_newline = false
