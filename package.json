    "bootstrap": "5.2.3"
  },
  "devDependencies": {
    "@smushytaco/rtlcss-webpack-plugin": "2.0.5",
    "autoprefixer": "10.4.21",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.2",
    "mini-css-extract-plugin": "2.9.2",
    "postcss": "8.5.6",
    "postcss-loader": "8.1.1",
    "rtlcss": "4.3.0",
    "sass-embedded": "1.89.2",
    "sass-loader": "16.0.5",
    "stylelint": "16.23.0",
    "stylelint-config-standard-scss": "15.0.1",
    "webpack": "5.101.0",
    "webpack-cli": "6.0.1",
    "webpack-merge": "6.0.1"
