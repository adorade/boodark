{
  "name": "boodark",
  "version": "1.2.0-dev",
  "description": "Bootstrap Dark theme for phpMyAdmin",
  "repository": "https://github.com/adorade/boodark.git",
  "author": "Adorade",
  "scripts": {
    "build": "yarn run build:prod",
    "build:dev": "webpack --progress --mode=development",
